<!doctype html>
<html>
  <head>
    <title>Registered Users</title>
  </head>
  <body>
    <h1>Registered Users</h1>

    <table border="1" cellpadding="8">
      <tr>
        <th>ID</th>
        <th>First</th>
        <th>Last</th>
        <th>Email</th>
        <th>Username</th>
      </tr>

      <% for (let i = 0; i < users.length; i++) { %>
        <tr>
          <td><%= users[i].id %></td>
          <td><%= users[i].first %></td>
          <td><%= users[i].last %></td>
          <td><%= users[i].email %></td>
          <td><%= users[i].username %></td>
        </tr>
      <% } %>

    </table>
  </body>
</html>
